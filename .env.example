# Database Configuration
DATABASE_URL=postgresql://postgres:your_password_here@localhost:5432/multi_agent_builder
DB_HOST=localhost
DB_PORT=5432
DB_NAME=multi_agent_builder
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_SSL=false
DB_POOL_MAX=20
DB_POOL_IDLE_TIMEOUT=30000
DB_POOL_CONNECTION_TIMEOUT=2000

# Test Database Configuration
# Used for integration tests - separate database from development
TEST_DATABASE_URL=postgresql://postgres:your_password_here@localhost:5432/multi_agent_builder_test
TEST_DB_HOST=localhost
TEST_DB_PORT=5432
TEST_DB_NAME=multi_agent_builder_test
TEST_DB_USER=postgres
TEST_DB_PASSWORD=your_password_here

# Anthropic API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022
ANTHROPIC_MAX_TOKENS=4096

# Application Configuration
NODE_ENV=development
PORT=3000
LOG_LEVEL=info

# Agent Configuration
DEFAULT_AGENT_BUDGET=10000
MAX_AGENT_DEPTH=5
AGENT_TIMEOUT=300000

# Interactive Session Configuration
ENABLE_INTERACTIVE_UI=true
WS_PORT=3001
SESSION_TIMEOUT=3600000
APPROVAL_TIMEOUT=300000

# Workspace Configuration
WORKTREE_BASE_PATH=./worktrees
WORKSPACE_CLEANUP_INTERVAL=3600000

# Message Queue Configuration
MESSAGE_QUEUE_POLL_INTERVAL=1000
MESSAGE_RETENTION_DAYS=7

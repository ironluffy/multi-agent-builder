{
  "name": "multi-agent-orchestrator",
  "version": "0.1.0",
  "description": "A sophisticated multi-agent orchestration system with collective intelligence capabilities",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "test": "vitest",
    "test:unit": "vitest tests/unit",
    "test:integration": "vitest tests/integration",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:setup": "tsx tests/setup/test-db-setup.ts setup",
    "test:teardown": "tsx tests/setup/test-db-setup.ts teardown",
    "test:reset": "tsx tests/setup/test-db-setup.ts reset",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "migrate": "node dist/database/migrate.js",
    "monitor:cli": "tsx src/cli/MonitorCLI.ts",
    "monitor:server": "tsx src/monitoring/MonitoringServer.ts",
    "demo": "tsx demo/simple-spawn-test.ts"
  },
  "keywords": [
    "multi-agent",
    "orchestration",
    "ai",
    "collective-intelligence",
    "distributed-systems"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.50",
    "@anthropic-ai/sdk": "^0.30.1",
    "dotenv": "^17.2.3",
    "pg": "^8.11.3",
    "pino": "^8.19.0",
    "pino-pretty": "^13.1.2",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/pg": "^8.11.0",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^4.0.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "5.3.3",
    "vitest": "^4.0.8"
  }
}
